; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="FF"
config/tags=PackedStringArray("jam")
run/main_scene="res://tests/test_scene.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/image="res://assets/icon.png"
config/icon="res://assets/icon.png"

[autoload]

GameEvents="*res://scripts/autoloads/game_events.gd"
ScreenMessage="res://scripts/autoloads/screen_message.tscn"
CounterAttacksUi="res://ui/counter_attacks_ui.tscn"
DamageNumbers="*res://scripts/autoloads/damage_numbers.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/resizable=false
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://assets/gui/cursor.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/project_time_tracker/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "green",
"res://entities/": "orange",
"res://entities/enemy/available enemies/": "red",
"res://entities/player/playable_characters/": "red",
"res://scripts/": "gray"
}

